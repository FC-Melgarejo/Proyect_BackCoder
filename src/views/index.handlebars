
  <h1>Catálogo de productos</h1>

   <table>
    <tr>
      <th>Título</th>
      <th>Descripción</th>
      <th>Precio</th>
      <th>Thumbnail</th>
      <th>Código</th>
    </tr>
    {{#each products}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td><img src="{{this.thumbnail}}" alt="Thumbnail" style="width: 50px;"></td>
      <td>{{this.code}}</td>
   <td>
  <img src="/uploads/{{this.thumbnail}}" alt="Thumbnail" style="width: 50px;">
  <button class="btn-delete" data-id="{{this.id}}">Eliminar</button>
</td>


    </tr>
    {{/each}}
  </table>

  



  


